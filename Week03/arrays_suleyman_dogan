import numpy as np

def replace_center_with_minus_one(d, n, m):
    if n < 0:
        return "Expected an error for negative n"
    if m < 0:
        return "Expected an error for negative m"
    if m > n:
        return "Expected an error when m > n"
    if d <= 0:
        return "Expected an error when d <= 0"

    min_value = 10 ** (d - 1)
    max_value = (10 ** d) - 1
    
    random_array = np.random.randint(min_value, max_value, (n, n))
    center_size = min(m, n)
    
    start_index = (n - center_size) // 2
    end_index = start_index + center_size
    random_array[start_index:end_index, start_index:end_index] = -1
    
    return random_array

